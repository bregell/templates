<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Screeps Launcher</Name>
  <Description>Screeps Launcher</Description>
  <Registry>https://hub.docker.com/r/screepers/screeps-launcher</Registry>
  <Repository>screepers/screeps-launcher</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>21025</HostPort>
        <ContainerPort>21025</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>21025</HostPort>
        <ContainerPort>21025</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/screeps</HostDir>
      <ContainerDir>/screeps</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon>https://avatars.githubusercontent.com/u/9197419?s=200&v=4</Icon>
  <ExtraParams></ExtraParams>
</Container>
